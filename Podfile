pod 'Firebase/Core'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'



  # Pods for IntDevTask

  target 'IntDevTaskTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'IntDevTaskUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
